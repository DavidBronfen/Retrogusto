{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "none", "varsIgnorePattern": "should"}],
    "arrow-body-style": 0,
    "import/no-dynamic-require": 0,
    "linebreak-style": 0
  }
}
